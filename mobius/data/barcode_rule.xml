<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="sequence_for_barcode" model="ir.sequence">
            <field name="name">mobius_sequence</field>
            <field name="implementation" eval="'standard'"/>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="product_generated_barcode_mobius" model="barcode.rule">
            <field name="barcode_nomenclature_id" ref="barcodes.default_barcode_nomenclature"/>
            <field name="name">Mobius</field>
            <field name="type">product</field>
            <field name="encoding">ean13</field>
            <field name="pattern">240.........</field>
            <field name="generate_type" eval="'sequence'"/>
            <field name="sequence_id" ref="sequence_for_barcode"/>
        </record>
    </data>
</odoo>
