<?xml version="1.0" encoding="UTF-8"?>
<odoo>
   <record id="lead_by_vat_wizard_form" model="ir.ui.view">
       <field name="name">lead_by_vat.wizard.form</field>
       <field name="model">lead_by_vat.wizard</field>
       <field name="arch" type="xml">
           <form string="Create Lead By Vat">
               <group>
                   <field name="vat_code"/>
               </group>
               <footer>
                   <button name="button_vat_code" string="Create by VAT" type="object" class="btn-primary"/>
                   <button string="Cancel" special="cancel" class="btn-secondary" />
               </footer>
           </form>
       </field>
   </record>
</odoo>