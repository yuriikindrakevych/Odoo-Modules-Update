<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="mobius_lead_condition_lead_condition_view_tree" model="ir.ui.view">
        <field name="name">crm.lead.condition.tree</field>
        <field name="model">crm.lead.condition</field>
        <field name="arch" type="xml">
            <tree string="Lead Conditions" editable="bottom">
                <field name="name"/>
                <field name="crm_team_id"/>
            </tree>
        </field>
    </record>

    <record id="mobius_lead_condition_lead_condition_action_window" model="ir.actions.act_window">
        <field name="name">Lead Conditions</field>
        <field name="res_model">crm.lead.condition</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>
