<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="mobius_lead_condition_crm_team_view_kanban_dashboard_inherit" model="ir.ui.view">
        <field name="name">crm.team.view.kanban.dashboard.inherit (mobius_lead_condition)</field>
        <field name="model">crm.team</field>
        <field name="inherit_id" ref="sales_team.crm_team_view_kanban_dashboard"/>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//div[@name='to_replace_in_sale_crm']" position="inside">
                    <button type="object" class="btn" name="action_open_lead_with_condition"><span>Lead Condition</span></button>
                </xpath>
            </data>
        </field>
    </record>

</odoo>
