<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="session_id_action_ir_cron" model="ir.cron">
            <field name="name">Session ID Cleaning</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="code">env['mobius.session'].scheduler_cleaning()</field>
        </record>
    </data>
</odoo>
